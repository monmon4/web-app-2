<h1>Login</h1>

<%= form_for(:account, url: sessions_path) do |f|%>
<div class="field"> <%= f.label :user_name %> <br/> <%= f.text_field :user_name%> </div>

<p/>

<div class="field"> <%= f.label :password%> <br/> <%= f.password_field :password%> </div>

<div class="actions"> <%= f.submit "login"%> </div> <%= link_to "signup", signup_path %>
<% end %>